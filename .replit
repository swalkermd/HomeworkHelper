modules = ["nodejs-22"]
[agent]
expertMode = true
integrations = ["javascript_openai_ai_integrations:1.0.0"]

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "expo-dev-server"

[[workflows.workflow]]
name = "expo-dev-server"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "bash -c \"PORT=8081 npx expo start --web --port 8081 & sleep 3 && npx tsx server/proxy.ts\""
waitForPort = 5000

[[ports]]
localPort = 3000
externalPort = 3002

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 8081
externalPort = 8081

[[ports]]
localPort = 8082
externalPort = 3001

[[ports]]
localPort = 33479
externalPort = 4200

[[ports]]
localPort = 36981
externalPort = 6000

[[ports]]
localPort = 37147
externalPort = 5173

[[ports]]
localPort = 38361
externalPort = 5000

[[ports]]
localPort = 40343
externalPort = 3003

[[ports]]
localPort = 42311
externalPort = 3000

[deployment]
deploymentTarget = "autoscale"
run = ["npx", "tsx", "server/proxy.ts"]
build = ["npx", "expo", "export", "--platform", "web"]
