modules = ["nodejs-22"]
[agent]
expertMode = true
integrations = ["javascript_openai_ai_integrations:1.0.0"]

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "expo-dev-server"

[[workflows.workflow]]
name = "expo-dev-server"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "bash -c \"npx tsx server/index.ts & PORT=5000 npx expo start --web --port 5000\""
waitForPort = 5000

[[ports]]
localPort = 3000
externalPort = 3002

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 33279
externalPort = 3001

[[ports]]
localPort = 38377
externalPort = 3000
